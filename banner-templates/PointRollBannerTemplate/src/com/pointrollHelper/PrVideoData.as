package com.pointrollHelper{	/**	 * @author jdutcher	 */	public class PrVideoData 	{		public var baseName : String;		public var instanceNumber : int;		public var duration : Number;		public var milestones : Array;				/**		 * PrVideoData constructor.		 * @param baseName - the base name of the video. assigned by the user (read: you) when encoding the video.		 * @param instanceNumber - the instance number of the video. Used for tracking.		 * @param duration - the duration, in seconds, of the video. Be as accurate as you can, and it will reduce problems with the PrVideo player component.		 */		public function PrVideoData(baseName : String, instanceNumber : int, duration : Number, milestones : Array = null)		{			this.baseName = baseName;			this.instanceNumber = instanceNumber;			this.duration = duration;			this.milestones = milestones;		}	}}