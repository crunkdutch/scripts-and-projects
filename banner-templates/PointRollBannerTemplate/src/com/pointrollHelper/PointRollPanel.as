package com.pointrollHelper{	/**	 * @author jdutcher	 */	public class PointRollPanel extends PointRollBase 	{		public function PointRollPanel() 		{			super();		}		protected function goPanel(panelNo : int) : void		{			pr.goPanel(panelNo);		}		protected function killPanel() : void		{			pr.close();		}		protected function pinSite() : void		{			pr.pin();		}	}}