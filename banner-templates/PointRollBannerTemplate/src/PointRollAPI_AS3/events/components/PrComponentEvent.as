/** * PointRoll Custom Event *  * Video Display notifications *  * @class 		PrComponentEvent * @package		ointRollAPI_AS3.events.components * @author		PointRoll */package PointRollAPI_AS3.events.components {	import flash.events.Event;	/** @private **/	public class PrComponentEvent extends Event {				public static const DEFAULT_NAME:String = "PointRollAPI_AS3.events.components.PRComponentEvent";				public static const INIT:String = "init";		public static const PLAYBACK_START:String = "playbackStart";		public static const PLAYLISTFINISHED:String = "playListFinished";		public static const PLAYLISTFIRST:String = "playListFirst";		public static const PLAYLISTLAST:String = "playListLast";		public static const VOLUME_CHANGE:String = "volumeChange";		public static const UNMUTE:String = "unmute";				public var params:Object;				public function PrComponentEvent(type:String, pms:Object = null, bubbles:Boolean = false, cancelable:Boolean = false) {			super(type, bubbles, cancelable);			this.params = pms;		}				public override function clone():Event {			return new PrComponentEvent(type, this.params, bubbles, cancelable);        }	}	} 